<% content_for :body do %>
  <div  class="container-fluid" id="projects">
    <% @projects.each do |project| %>
    <div class="row no-gutter">
      <div class="col-sm-8">
        &nbsp;
      </div>
      <div class="col-sm-2"><%= link_to project.name, refinery.projects_project_path(project) %></div>
      <div class="col-sm-2 works-img"><%= image_tag(project.img_home.url, class:"img-responsive") %></div>
    </div><!-- /row -->
  </div><!-- /container -->
<% end %>
<% end %>


<%= render '/refinery/content_page' %>
